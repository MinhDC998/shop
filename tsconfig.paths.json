{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@src/*": ["./*"],
            "@components/*" : ["src/app/components/*"],
            "@routes/*": ["src/app/routers/*"],
            "@public/*": ["src/public/*"],
            "@client/*": ["src/app/components/client/*"],
            "@admin/*": ["src/app/components/admin/*"],
            "@images/*": ["src/public/assets/images/*"],
            "@utils/*": ["src/app/utils/*"]
         }
    }
}